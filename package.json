{
  "name": "boilerroom",
  "version": "1.1.0",
  "description": "A universal boilerplate for es6 modules",
  "repository": "https://github.com/craig-mulligan/boilerroom",
  "main": "dist/index.js",
  "scripts": {
    "test": "babel-node ./node_modules/.bin/_mocha",
    "lint": "eslint src/**",
    "docs": "jsdoc2md 'src/**/*.js' > DOCUMENTATION.md",
    "build": "babel src --out-dir dist --copy-files && npm run build-browser",
    "build-browser": "webpack -p dist/index.js dist/bundle.js --output-library boilerroom",
    "version": "versionist",
    "prepublish": "npm run build"
  },
  "keywords": [
    "boilerplate",
    "es6",
    "babel",
    "webpack",
    "universal"
  ],
  "author": "craig-mulligan",
  "browser": {},
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "6.23.0",
    "babel-preset-es2015": "6.22.0",
    "eslint": "3.15.0",
    "eslint-config-semistandard": "7.0.0",
    "eslint-config-standard": "6.2.1",
    "eslint-plugin-promise": "3.4.2",
    "eslint-plugin-standard": "2.0.1",
    "lodash": "4.17.4",
    "mocha": "3.2.0",
    "mochainon": "1.0.0",
    "versionist": "2.8.1",
    "webpack": "2.2.1"
  }
}
